@model Packege

<head>
  <meta charset="utf-8" />
  <title>Destino Certo - Registro De Pacotes</title>
  <link rel="stylesheet" href="~/css/site.css" />
</head>
<header>

  <h2>Cadastro de Pacotes</h2>
  <div class="FormPkg">

    <p id="pMensagem">@ViewBag.Mensagem</p>
    <form asp-action="Cadastrar" method="POST">
      <p>
        <label asp-for="nome">Nome:</label>
        <input asp-for="nome" />
      </p>
      <p>
        <label asp-for="origem">Origem:</label>
        <input asp-for="origem">
      </p>
      <p>
        <label asp-for="destino">Destino:</label>
        <input asp-for="destino">
      </p>
      <p>
        <label asp-for="atrativos">Atrativos:</label>
        <input asp-for="atrativos">
      </p>
      <p>
        <label asp-for="saida">Saida:</label>
        <input asp-for="saida" type="date">
      </p>
      <p>
        <label asp-for="retorno">Retorno:</label>
        <input asp-for="retorno" type="date">
      </p>
      <p>
        <input href="H" type="submit" value="Cadastrar" />
      </p>
    </form>
  </div>
</header>